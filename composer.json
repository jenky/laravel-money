{
    "name": "jenky/laravel-money",
    "type": "library",
    "description": "Laravel wrapper for brick/money",
    "keywords": [
        "jenky",
        "laravel",
        "money",
        "laravel-money"
    ],
    "homepage": "https://github.com/jenky/laravel-money",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "contact@lynh.me",
            "homepage": "https://github.com/jenky",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "brick/money": "^0.4.4"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7|^4.0|^5.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\LaravelMoney\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\LaravelMoney\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
